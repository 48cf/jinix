name=xeyes
from_source=xeyes
revision=1
hostdeps="gcc pkg-config"
deps="core-libs libx11 libxmu libxaw libxrender libxft libxt libxi libxkbfile"

build() {
    autotools_configure \
        --disable-selective-werror

    make -j${parallelism}
}

package() {
    DESTDIR="${dest_dir}" make install-strip
}
