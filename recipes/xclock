name=xclock
from_source=xclock
revision=1
builddeps="base"
deps="core-libs libx11 libxmu libxaw libxrender libxft libxt libxkbfile"

build() {
    autotools_configure \
        --disable-selective-werror

    make -j${parallelism}
}

package() {
    DESTDIR="${dest_dir}" make install

    post_package_strip
}
